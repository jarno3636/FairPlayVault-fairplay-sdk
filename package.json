{
  "name": "@fairplayvault/sdk",
  "version": "0.1.0",
  "description": "Typed SDK for interacting with FairplayVault commit-reveal pools on Base.",
  "type": "module",
  "files": ["dist"],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "fairplay",
    "sdk",
    "base",
    "usdc",
    "commit-reveal",
    "crypto",
    "web3"
  ],
  "author": "Fairplay Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jarno3636/fairplay-sdk"
  },
  "peerDependencies": {
    "viem": ">=2.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "viem": "^2.13.0"
  }
}
